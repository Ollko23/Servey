</form>

<h2>Results: </h2>

<% responses.forEach(r=>{ %>
    <span>
        <%= r.id%>:
    </span><span>
        <%= r.answer1%>
            <%= r.answer2%>
                <%= r.answer3%>
                    <%= r.answer4%>
                        <%= r.answer5%>
                            <%= r.answer6%>
                                <%= r.answer7%>
                                    <%= r.answer8%>
    </span><br>
    <% }) %>