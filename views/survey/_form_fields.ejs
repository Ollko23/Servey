<%if(locals.errorMessage !=null){%>
    <%=locals.errorMessage%>
        <%} %>
            <div class="wrapper">
                <div class="contrainer">

                    <div class="question">
                        <p>Jakiego języka obecego jako drugiego uczysz się obecnie w szkole?</p>
                        <div class="answers">
                            <% for(const question of questions.question1){ %>
                                <label>
                                    <input name="1" type="radio" value="<%=question %>" required>
                                    <span>
                                        <%=question%>
                                    </span></input>
                                </label>
                                <%}%>
                        </div>
                    </div>

                    <div class="question">
                        <p>Jakiego języka obcego jako drugiego chiałbyć/chciałabyś się uczyć najbardziej gdybyś
                            miał/miała
                            nieograniczony wybór?
                        </p>
                        <div class="answers">
                            <% for(const question of questions.question2){ %>
                                <label>
                                    <input name="2" type="radio" value="<%=question %>" required>
                                    <%=question%></input>
                                </label>
                                <%}%>
