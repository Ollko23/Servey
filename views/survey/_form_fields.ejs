                        <div class="answers fat">
                            <% for(const question of questions.question7){ %>
                                <label>
                                    <input name="7" type="radio" value="<%=question %>" required>
                                    <span>
                                        <%=question%>
                                    </span> </input>
                                </label>
                                <%}%>
                        </div>
                    </div>

                    <div class="question ">
                        <p>Co sprawia, że dany język wydaje Ci się
                            atrakcyjny?</p>
                        <div class="answers fat">
